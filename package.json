{
  "name": "szdt-admin-components",
  "version": "1.0.3",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "updateversion": "npm version patch --no-git-tag-version",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint-staged": "lint-staged",
    "postversion": "git add package.json"
  },
  "files": [
    "dist"
  ],
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged && npm run updateversion"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "@umijs/yorkie": "^2.0.5",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1"
  }
}
